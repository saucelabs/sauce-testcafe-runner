{
  "apiVersion": "v1alpha",
  "docker": {
    "fileTransfer": "copy",
    "image": "saucelabs/stt-testcafe-node:local"
  },
  "kind": "testcafe",
  "sauce": {
    "concurrency": 1,
    "metadata": {
      "build": "Release $CI_COMMIT_SHORT_SHA",
      "name": "Testing Testcafe Support",
      "tags": [
        "e2e",
        "release team",
        "other tag"
      ]
    },
    "region": "us-west-1"
  },
  "suites": [
    {
      "browserName": "chrome",
      "name": "saucy test",
      "speed": 1,
      "src": [
        "*/*.test.js"
      ]
    }
  ],
  "testcafe": {
    "projectPath": "tests/",
    "version": "1.10.1"
  }
}
