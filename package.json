    "lodash": "4.17.21",
    "mocha-junit-reporter": "^2.2.1",
    "sauce-testrunner-utils": "3.3.0",
    "shelljs": "^0.8.5",
    "testcafe": "3.7.2",
    "testcafe-browser-provider-ios": "0.8.1",
    "testcafe-reporter-saucelabs": "3.6.0",
    "typescript": "^5.7.3",
    "xml-js": "1.6.11",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
