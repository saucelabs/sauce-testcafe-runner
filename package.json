    "@types/node": "^22.10.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-testcafe": "^0.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "release-it": "^18.1.1",
    "ts-jest": "^29.2.5",
    "typescript-eslint": "^8.21.0"
  }
